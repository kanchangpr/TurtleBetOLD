package com.jetbet.dao;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import com.jetbet.bean.SportsBean;
import com.jetbet.repository.SportsRepository;

@Repository
public class BetfairDao {
	
	@Autowired
	SportsRepository sportsRepository;

	public List<SportsBean> storeListOfEventTypesInDB(List<SportsBean> sportsBeanList) {
		return sportsRepository.saveAll(sportsBeanList);
	}

}
